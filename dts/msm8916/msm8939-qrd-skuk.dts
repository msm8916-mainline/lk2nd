	bq-piccolo {
		model = "BQ Aquaris M5";
		compatible = "bq,piccolo", "qcom,msm8939", "lk2nd,device";
		lk2nd,match-cmdline = "* mdss_mdp.panel=1:dsi:0:qcom,mdss_dsi_truly_nt35695_1080p_cmd:1:none";
	};

	lenovo-pb1770m {
		model = "Lenovo PHAB Plus";
