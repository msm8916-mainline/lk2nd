		qcom,msm-id = <QCOM_ID_MSM8916 0>;
		qcom,board-id = <0xCE08FF01 0>;

		muic-reset {
			compatible = "samsung,muic-reset";
			i2c-reg = <0x25>;
			i2c-sda-gpios = <&tlmm 105 I2C_GPIO_FLAGS>;
			i2c-scl-gpios = <&tlmm 106 I2C_GPIO_FLAGS>;
		};
	};

