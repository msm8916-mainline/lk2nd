		qcom,mdss_dsi_otm1901a_tm_1080p_video {
			compatible = "wingtech,tm-otm1901a";
		};
		qcom,mdss_dsi_nt35596_1080p_video {
			compatible = "wingtech,nt35595";
		};
	};

};

