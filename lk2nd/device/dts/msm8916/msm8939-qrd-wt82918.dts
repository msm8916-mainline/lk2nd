};

&lk2nd {
	model = "Lenovo Vibe K5 (Wingtech WT82918)";
	compatible = "wingtech,wt82918";

	//FIXME: lk2nd,dtb-files = "msm8939-wingtech-wt82918", "msm8929-wingtech-wt82918hd";

